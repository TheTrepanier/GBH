  "bugs": {
    "url": "https://github.com/TheTrepanier/GBH/issues"
  },
  "homepage": "https://github.com/TheTrepanier/GBH#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "mysql2": "^1.7.0",
    "sequelize": "^5.19.0"
  }
}
